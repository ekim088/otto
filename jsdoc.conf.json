{
	"source": {
		"include": ["./src/", "./src/lib"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": ".+\\.(test|spec).js"
	},
	"opts": {
		"destination": "./jsdoc/",
		"recurse": true
	},
	"plugins": ["plugins/markdown", "node_modules/jsdoc-babel"],
	"babel": {
		"extensions": ["js", "jsx"],
		"ignore": ["**/*.(test|spec).js"],
		"babelrc": true,
		"presets": []
	}
}
